    <Script type="xscript5.1"><![CDATA[this.Form_MemberBlackList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.fn_selectBlackList();
};


//블랙 리스트 조회
this.fn_selectBlackList= function(){
	
	var strSvcID = "selectBlackList"
	var setURL = "svc::/selectBlackListByAdmin.do?time=" + new Date().getTime();
	var strInDatasets = "ds_search=ds_search";
	var strOutDatasets = "ds_list=ds_list";
	var strArg = "";
	var callBack = "fn_callBack";
	var inAsync = true;
	
	this.transaction(strSvcID,setURL,strInDatasets,strOutDatasets,strArg,callBack,inAsync);
}


// 공통 콜백
this.fn_callBack = function(svcID, errorCode, errorMSG){
	
	if(errorCode == -1){
		this.alert(errorMSG)
	}
	
	switch(svcID){
	case "selectBlackList" : 
		console.log(this.ds_list.saveXML());
		break;
		
	}
	
}

//검색하기
this.search_btn_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.fn_selectBlackList();
};

//초기화
this.Button01_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.reload();
};

//블랙리스트 팝업
this.black_list_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	//this.alert(e.cell)
	
	if(e.cell == 8){
		
		var memberId = this.ds_list.getColumn(e.row, "MEMBER_ID");
		
		var surl = "member::Form_MemberBlackListDetail.xfdl";
		var param = { MEMBER_ID: memberId };
		
		// 디버깅용 로그 추가
		trace("팝업 호출 시 전달할 MEMBER_ID: " + memberId);
		
		var popup = new nexacro.ChildFrame();
		popup.init("updatePop", 0, 0, 800, 700, null, null, surl);
		popup.set_dragmovetype("all");
		popup.set_showtitlebar(true);
		popup.set_titletext("블랙리스트 상세보기");
		
		// 두 번째 인자로 param을 전달
		popup.showModal(this.getOwnerFrame(), param, this, "fn_popCallback");
		
	}
};

//팝업 콜백
this.fn_popCallback = function(svcID, strVal){
	switch(svcID){
	case "updatePop":
		this.fn_selectBlackList();
		break;
	}
};

//이름 검색하면 바로 조회
this.Edit00_onkeyup = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13){
		this.fn_selectBlackList();
	}
};

//아이디 검색하면 바로 조회
this.Edit00_00_onkeyup = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13){
		this.fn_selectBlackList();
	}
};

//전화번호 검색하면 바로 조회
this.MaskEdit00_onkeyup = function(obj:nexacro.MaskEdit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13){
		this.fn_selectBlackList();
	}
};

//시작일
this.Calendar00_onchanged = function(obj:nexacro.Calendar,e:nexacro.ChangeEventInfo)
{
    // Div 안의 컴포넌트 참조 수정
    var startDate = this.black_search_box.form.Calendar00.value;
    var endDate   = this.black_search_box.form.Calendar01.value;
    
    if (!startDate) return;
    
    if (endDate && endDate < startDate) {
        this.alert("종료일은 시작일보다 빠를 수 없습니다.");
        this.black_search_box.form.Calendar01.set_value(startDate);
        endDate = startDate;
    }
    
    this.ds_search.setColumn(0, "START_INPUT_DT", startDate + "000000");
    
    if (endDate) {
        this.ds_search.setColumn(0, "END_INPUT_DT", endDate + "235959");
    }
    
    this.fn_selectBlackList()
};

//종료일
this.Calendar01_onchanged = function(obj:nexacro.Calendar,e:nexacro.ChangeEventInfo)
{
    // Div 안의 컴포넌트 참조 수정
    var startDate = this.black_search_box.form.Calendar00.value;
    var endDate   = this.black_search_box.form.Calendar01.value;
    
    if (!endDate) return;
    
    if (startDate && endDate < startDate) {
        this.alert("종료일은 시작일보다 빠를 수 없습니다.");
        this.black_search_box.form.Calendar01.set_value(startDate);
        endDate = startDate;
    }
    
    this.ds_search.setColumn(0, "END_INPUT_DT", endDate + "235959");
    
    if (startDate) {
        this.ds_search.setColumn(0, "START_INPUT_DT", startDate + "000000");
    }
    
    this.fn_selectBlackList();
};
]]></Script>
